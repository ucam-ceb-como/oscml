{
"numerical_settings":{
	"seed": 1,
    "cudnn_deterministic": true,
    "cudnn_benchmark": false
},
"dataset":{
    "src": "./data/processed/HOPV_15_revised_2_processed_homo.csv",
    "z-stand": "False",
    "x_column": ["smiles"],
    "y_column": ["pce"],
	"split": [200, null, 36]
    },
"model":{
    "name": "AttentiveFP",
	"model_specific":{
		"graph_feat_size": {"type":"int","low":16,"high":256},
		"num_layers":{"type":"int","low":1,"high":6},
		"num_timesteps":{"type":"int","low":1,"high":4},
		"dropout":{"type":"uniform","low":0.0,"high":0.3}
		},
	"featurizer": "full"
	},
"training":{
	"optimiser":{
		"name":"Adam",
		"lr":{"type":"loguniform","low":1e-5,"high":1e-2},
		"weight_decay":{"type":"loguniform","low":1e-5,"high":0.01}
		},
	"batch_size": 40,
	"epochs": 1,
	"patience": -1,
	"metric": "val_loss",
	"direction": "minimize",
	"cross_validation": false
    }
}
