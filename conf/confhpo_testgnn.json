{
"numerical_settings":{
	"seed": 1,
    "cudnn_deterministic": true,
    "cudnn_benchmark": false
},
"dataset":{
    "src": "./data/processed/CEPDB_25000.csv",
    "z-stand": "False",
    "x_column": ["SMILES_str"],
    "y_column": ["pce"]
    },
"model":{
    "name": "SimpleGNN",
    "model_specific":{
        "embedding_dim": 256,
        "param_1": 1,
		"param_2": {"starting_value":4},
		"param_3": {"starting_value":"param_1"},
		"param_4": [1,3,4],
		"param_5": {"values":[1,3,4],"length":100},
		"param_6": {"values":[1,3,4],"length":"param_3"},
		"param_7": {"type":"int","low":10,"high":12, "starting_value":"embedding_dim", "length":4},
		"param_8": {"type":"int","low":1,"high":4, "starting_value":"param_4"},
		"param_9": {"type":"int","low":1,"high":4, "starting_value":"param_4", "length":10},
        "param_10": {"type":"int","low":1,"high":4, "starting_value":"param_9[2]", "length":"param_7"},
        "param_11": {"type":"int","low":1,"high":14, "starting_value":"param_1", "length":10, "direction":"constant"},
        "param_12": {"type":"int","low":1,"high":14, "starting_value":4, "length":10, "direction":"increasing"},
        "param_13": {"type":"int","low":1,"high":14, "starting_value":4, "length":10, "direction":"decreasing"},
        "param_14": {"type":"int","low":1,"high":14, "starting_value":"param_5[2]", "length":"param_4", "direction":"decreasing"},
        "param_15": {"type":"int","low":1,"high":14, "starting_value":"param_5[2]", "length":"param_4[2]", "direction":"decreasing"},

        "conv_layers":{"type":"int","low":1,"high":4},
        "conv_dims":{"type":"int","low":10,"high":15,"direction":"constant","length":"conv_layers","starting_value":"embedding_dim"},
        "mlp_layers":{"type":"int","low":1,"high":15},
        "mlp_units":{"type":"int","low":5,"high":15,"length":"mlp_layers","direction":"constant"},
        "mlp_dropouts":{"type":"uniform","low":0.1,"high":0.3,"length":"mlp_layers","direction":"constant"}
        },
	"type_dict": "CEP25000"
    },
"training":{
	"optimiser":{
		"name":"Adam",
		"lr":{"type":"loguniform","low":1e-5,"high":1e-2},
		"weight_decay":{"type":"loguniform","low":1e-5,"high":0.01}
		},
	"batch_size": 250,
	"epochs": 10,
	"patience": -1,
	"metric": "val_loss"
    }
}
