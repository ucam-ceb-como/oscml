{
  "numerical_settings": {
    "seed": 1,
    "cudnn_deterministic": true,
    "cudnn_benchmark": false
  },
  "dataset": {
    "src": "./data/processed/CEPDB_25000.csv",
    "z-stand": "False",
    "x_column": ["SMILES_str"],
    "y_column": ["pce"],
    "split": "ml_phase"
  },
  "model": {
    "name": "BILSTM",
    "model_specific": {
      "embedding_dim": {"type": "categorical", "choices": [8, 16, 32, 64, 128, 256]},
      "mlp_layers": {"type": "int", "low": 1, "high": 4},
      "mlp_units": {"type": "int", "low": 8, "high": 256, "starting_value": "embedding_dim", "length": "mlp_layers", "direction": "decreasing"},
      "mlp_dropouts": {"type": "uniform", "low": 0.0, "high": 0.3, "direction": "constant", "length": "mlp_layers" }
    },
    "max_sequence_length": 160,
    "type_dict": "CEP25000"
  },
  "training": {
    "optimiser": {
      "name": "Adam",
      "lr": {"type": "loguniform", "low": 1e-5, "high": 1e-2},
      "weight_decay": {"type": "loguniform", "low": 1e-5, "high": 0.01}
    },
    "batch_size": 250,
    "epochs": 400,
    "patience": 20,
    "min_delta": 0.0,
    "metric": "val_loss",
    "direction": "minimize",
    "cross_validation": false,
    "study_name": "bilstm_cep",
    "storage": "sqlite:///bilstm_cep.db",
    "__remove_this_bit__storage_timeout": 1000,
    "load_if_exists": true,
    "n_trials": 0,
    "__remove_this_bit__n_jobs": 20
  },
  "post_processing": {
    "contour_plot": false,
    "best_trial_retraining": false,
    "z_transform_inverse_prediction": false,
    "regression_plot": false,
    "transfer": true
  },
  "transfer_learning": {
    "fine_tune": false,
    "freeze_and_train": true,
    "ckpt_path": "./transfer_learning/bilstm/best_trial_retrain_model-v0.ckpt",
    "dataset": {
      "src": "./data/processed/HOPV_15_revised_2_processed_homo_5fold.csv",
      "z-stand": "False",
      "x_column": ["smiles"],
      "y_column": ["pce"],
      "split": "ml_phase_fold_3"
    }
  }
}