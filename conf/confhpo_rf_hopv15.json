{
"numerical_settings":{
	"seed": 1,
    "cudnn_deterministic": true,
    "cudnn_benchmark": false
},
"dataset":{
    "src": "./data/processed/HOPV_15_revised_2_processed_homo.csv",
    "z-stand": "False",
    "x_column": ["smiles"],
    "y_column": ["pce"],
	"split": "ml_phase"
    },
"model":{
    "name": "RF",
	"model_specific":{
		"n_estimators":{"type":"int","low":16,"high":256},
		"max_depth":{"type":"int","low":10,"high":100},
		"min_samples_split":{"type":"int","low":2,"high":5},
		"min_samples_leaf":{"type":"int","low":1,"high":5},
		"max_features":{"type":"categorical","choices":[0.05, 0.1, 0.5, 1.0]},
		"bootstrap": {"type":"categorical","choices":[true, false]},
		"max_samples":{"type":"int","low":5,"high":50}
	},
	"fingerprint_specific":{
		"type":"morgan",
		"nBits":{"type":"categorical","choices": [128, 256, 512, 1024, 2048, 4096]},
		"radius": {"type":"categorical","choices": [2, 3, 4, 5]},
		"useChirality": {"type":"categorical","choices":[true, false]},
		"useBondTypes":{"type":"categorical","choices":[true, false]}
		}
    },
"training":{
    "criterion": "mse",
	"cross_validation": false,
	"direction": "minimize",
	"__remove_this_bit__study_name": "rf_hopv15_homo",
	"__remove_this_bit__storage": "sqlite:///rf_hopv15_homo.db",
	"__remove_this_bit__load_if_exists": true,
	"__remove_this_bit__n_trials": 50,
	"__remove_this_bit__n_jobs": 1
    }
}