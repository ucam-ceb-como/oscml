{
"dataset":{
    "src": "./data/processed/CEP25000.csv",
    "z-stand": "False",
    "x_column": ["smiles"],
    "y_column": ["pce"]
    },
"model":{
    "name": "AttentiveFP",
	"model_specific":{
		"graph_feat_size": {"type":"int","low":16,"high":256},
		"num_layers":{"type":"int","low":1,"high":6},
		"num_timesteps":{"type":"int","low":1,"high":4},
		"dropout":{"type":"uniform","low":0.0,"high":0.3}
		},
	"training_specific":{
		"opt_name":"Adam",
		"lr":{"type":"loguniform","low":1e-5,"high":1e-2},
		"epochs": 300,
		"batch_size": 40
		},
	"dataloader":{
		"type_dict": "label"
	   }
	}
}
